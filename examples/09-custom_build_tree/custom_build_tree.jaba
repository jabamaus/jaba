cpp :AppWithCustomisedBuildTree do
  platforms [:windows_x86_64]
  project do
    type :console
    configs [:Debug, :Release]
    src ['main.cpp']
  end
  config do
    bindir "custom_build_tree/bin/#{config}"
    libdir "custom_build_tree/lib/#{config}"
    objdir "custom_build_tree/obj/#{config}"
  end
end
